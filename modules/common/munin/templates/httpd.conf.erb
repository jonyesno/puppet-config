<VirtualHost *:80>
  ServerName munin.<%= domain %>
  RewriteEngine On
  RewriteRule ^/(.*) https://%{HTTP_HOST}/$1 [L,R=301]
</VirtualHost>

<VirtualHost *:443>
  SSLEngine On
  SSLCertificateFile   /etc/httpd/ssl/star.<%= domain %>.pem
  SSLCACertificateFile /etc/httpd/ssl/ca.pem
  ServerName munin.<%= domain %>
  DocumentRoot /var/www/html/munin
</VirtualHost>

